<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="event_transmission_search" model="ir.ui.view">
        <field name="name">l10n_br_fiscal.event.transmission.search</field>
        <field name="model">l10n_br_fiscal.event.transmission</field>
        <field name="arch" type="xml">
            <search string="Fiscal Event Transmission">
                <field name="event_id" />
            </search>
        </field>
    </record>

    <record id="event_transmission_tree" model="ir.ui.view">
        <field name="name">l10n_br_fiscal.event.transmission.tree</field>
        <field name="model">l10n_br_fiscal.event.transmission</field>
        <field name="arch" type="xml">
            <tree edit="false" create="false">
                <field name="event_id" />
                <field name="create_date" />
            </tree>
        </field>
    </record>

    <record id="event_transmission_form" model="ir.ui.view">
        <field name="name">l10n_br_fiscal.event.transmission.form</field>
        <field name="model">l10n_br_fiscal.event.transmission</field>
        <field name="arch" type="xml">
            <form string="Fiscal Event Transmission" edit="false" create="false">
                <header>
                    <field name="state" widget="statusbar" />
                </header>
                <sheet>
                    <group>
                        <field name="event_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
