    <record id="group_manager" model="res.groups">
        <field name="name">Foreign Trade Manager</field>
        <field name="category_id" ref="module_category_l10n_br_di" />
        <field name="implied_ids" eval="[(4, ref('l10n_br_di.group_user'))]" />
    </record>

</odoo>
