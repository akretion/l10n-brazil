<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="module_category_l10n_br_di" model="ir.module.category">
        <field name="name">Brazilian Foreign Trade</field>
        <field name="description">Create Import Custom Declaration documents</field>
        <field name="sequence">30</field>
    </record>

    <record id="group_user" model="res.groups">
        <field name="name">Foreign Trade User</field>
        <field name="category_id" ref="module_category_l10n_br_di" />
